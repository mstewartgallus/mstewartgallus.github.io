---
layout: none
---
{
    "urls": [
        {%- for post in site.posts -%}
        {{ post.url | jsonify }}
        {%- unless forloop.last -%},{%- endunless -%}
        {%- endfor -%}
    ],
    "dates": [
        {%- for post in site.posts -%}
        "{{ post.date | date: "%Y-%m-%d" }}"
        {%- unless forloop.last -%},{%- endunless -%}
        {%- endfor -%}
    ]
}
